grammar org.xtext.example.mydsl.Profissional with org.eclipse.xtext.common.Terminals

generate profissional "http://www.xtext.org/example/mydsl/Profissional"

Model:
	profissional+=Pessoa*;
	
Pessoa:
	'Nome:' name=ID ','
	empresa+=Empresa;
	
Empresa:
	'Empresa:' name=ID ',Linguagens:{'
	linguagens+=Linguagens* '}';

Linguagens:
	name=ID;